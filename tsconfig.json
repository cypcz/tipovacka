{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["es2019"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6",
    "outDir": "dist",
    "resolveJsonModule": true
  },
  "exclude": ["dist", "./node_modules"],
}
